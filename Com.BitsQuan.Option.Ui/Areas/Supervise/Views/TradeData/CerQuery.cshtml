@model IEnumerable<Com.BitsQuan.Option.Core.ContractExecuteRecord>

<input type="hidden" id="ajaxpagecount" value="@(((Com.BitsQuan.Option.Ui.Models.Query.QueryEngine)ViewData["args"]).PageCount)" />

<table class="table table-striped">
    <tr>
        <th>编号</th>
        <th>用户</th>
        <th>合约</th>
        <th>持仓类型</th>
        <th>持仓数量</th>
        <th>行权基准价</th>
        <th>收钱付钱</th>
        <th>实际金额</th>
        <th>执行时间</th>
    </tr>
    @if (Model != null)
    {
        foreach (var v in Model)
        {
            <tr>
                <td>
                    @v.Id
                </td>
                <td>@v.Trader.Name</td>
                <td>@v.Contract.Name</td>
                <td>@v.PosType</td>
                <td>@v.Count</td>
                <td>@v.BasePrice</td> 
                <td>@v.IsAddTo</td>
                <td>
                @if(v.IsAddTo==true)
                {
                    @v.Total;
                }else
                    {
                         @("-"+v.Total);
                    }
               </td>
                <td>@v.When</td>
            </tr> 
        
        }
    }
</table> 
