@model IEnumerable<Com.BitsQuan.Option.Core.PositionSummaryData>

<input type="hidden" id="ajaxpagecount" value="@(((Com.BitsQuan.Option.Ui.Models.Query.QueryEngine)ViewData["args"]).PageCount)">

<table class="table table-striped">
    <tr>
        <th>编号</th>
        <th>用户</th>
        <th>时间</th>
        <th>合约名称</th>
        <th>合约代码</th>
        <th>持仓类型</th>
        <th>数量</th>
        <th>平仓盈亏</th>
        <th>成本价</th>
        <th>成本</th>
        <th>浮动盈亏</th>
        <th>保证率</th>
        <th>合约市值</th>
        <th>下单类型</th>
       
    </tr>
    @if (Model != null)
    {
        foreach (var v in Model)
        {
            <tr>
                <td>@v.Id</td>
                <td>@v.Trader.Name</td>
                <td>@v.When</td>
                <td>@v.Contract.Name</td>
                <td>@v.Contract.Code</td>
                <td>@v.PositionType</td>
                <td>@v.Count</td>
                <td>@v.ClosableCount</td>
                <td>@v.BuyPrice</td>
                <td>@v.BuyTotal</td>
                <td>@v.FloatProfit</td>
                <td>@v.Maintain</td>
                <td>@v.TotalValue</td>
                <td>@v.OrderType</td>
            </tr>
        }
    }

</table>