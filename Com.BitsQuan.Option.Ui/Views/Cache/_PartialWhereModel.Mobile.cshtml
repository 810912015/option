@model Com.BitsQuan.Option.Ui.Models.WhereModel

<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(a => a.StartTime, new { @class = "col-md-4 control-label" })
        <div class="col-md-8">
            @Html.TextBoxFor(a => a.StartTime, new { id = ViewData["sid"], @class = "col-md-8 form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(a => a.EndTime, new { @class = "col-md-4 control-label" })
        <div class="col-md-8">
            @Html.TextBoxFor(a => a.EndTime, new { id = ViewData["eid"], @class = "col-md-8 form-control" } )
        </div>
    </div>
    @if (ViewData["list"] != null)
    {
        <div class="form-group">
            @Html.LabelFor(a => a.Type, new { @class = "col-md-4 control-label" })
            <div class="col-md-8">
                @Html.DropDownListFor(a => a.Type, (List<SelectListItem>)ViewData["list"], new { @class = "col-md-8 form-control" })
            </div>
        </div>
    }
    <div class="form-group">
        <div class="col-md-4">
            <input type="submit" class="btn btn-primary"   value="确定">
        </div>
    </div>
</div>